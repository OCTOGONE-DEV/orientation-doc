SELECT UNIE.UNIE_OBJ_ID
CADO.VW_PORTAIL_EPS_UNIE UNIE INNER JOIN CADO.VW_PORTAIL_EPS_NIVE NIVE ON NIVE.NIVE_OBJ_ID = UNIE.NIVE_OBJ_ID;

# UNI EXEMPLE = = = =
SELECT UNIE.UNIE_OBJ_ID, UNIE.UNIE_NOM_LG, UNIE.UNIE_ECTS, UNIE.UNIE_DIPL, UNIE.UNIE_DFER, UNIE.NIFO_OBJ_ID, UNIE.NIVE_OBJ_ID, UNIE.UNIE_CODE , NIVE.NIVE_NOM FROM CADO.VW_PORTAIL_EPS_UNIE UNIE INNER JOIN CADO.VW_PORTAIL_EPS_NIVE NIVE ON NIVE.NIVE_OBJ_ID = UNIE.NIVE_OBJ_ID;

# ORIENTATION
SELECT ENSEIGN.NS_ETAB_ID, ENSEIGN.TE_INTITULE, ENSEIGN.DT_DEBUT, ENSEIGN.DT_FIN,ETAB.NOM_ETAB, ETAB.ADRS_IMPL, ETAB.CP_LOC_COM_IMPL, ETAB.NUMERO_FASE_ETAB FROM GOS2.VW_STRUCTURES_AUTORISEES ENSEIGN INNER JOIN FASE.VW_PORTAIL_ORIENTATION ETAB ON ETAB.NUMERO_FASE_ETAB = ENSEIGN.NS_ETAB_ID ;

# ORIENTATION TEST




SELECT ENSEIGN.NS_ETAB_ID, ENSEIGN.TE_INTITULE, ENSEIGN.DT_DEBUT, ENSEIGN.DT_FIN,ETAB.NOM_ETAB, ETAB.ADRS_IMPL, ETAB.CP_LOC_COM_IMPL, ETAB.NUMERO_FASE_ETAB 
FROM 
GOS2.VW_STRUCTURES_AUTORISEES ENSEIGN 
INNER JOIN 
FASE.VW_PORTAIL_ORIENTATION ETAB 
ON 
ETAB.NUMERO_FASE_ETAB = ENSEIGN.NS_ETAB_ID;

